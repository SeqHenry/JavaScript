/**********************************************************************************************/

                             //Program to showcase JS callstack

/**********************************************************************************************/

function ceo(){
console.log("I am in ceo")    
cto ()
}

function cto(){
    console.log("I am in cto")
    vpengg()
}

function vpengg(){
    console.log("I'm in vpengg")
    techlead()
}

function techlead(){
    console.log("I'm in techlead")
    developer()
}

function developer(){
    console.log("I'm in developer")
    throw new console.error("Something went headwire"); //throwing an error to show call stack in console
    
}

ceo()
